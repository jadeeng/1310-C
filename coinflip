using namespace std;
using std::time;

int main()
{
int face=0, number=1;
srand(time(0));
for(int i=1; i<=10000;i++){
    face=1+(rand()%2);
    if(i==1000){
    if(face==0){
        cout<<"Heads.";
    }
    else{
        cout<<"Tails.";
    }
    }
}

    return 0;
}
